{
  "name": "kekkai",
  "version": "0.1.0",
  "description": "javascript sandbox.",
  "main": "lib/index.js",
  "jsnext:main": "src/index.ts",
  "types": "lib",
  "type-check": "tsc",
  "scripts": {
    "test": "cross-env BROWSER=Chrome karma start karma.conf.js",
    "test-jsdom": "cross-env BROWSER=jsdom karma start karma.conf.js --singleRun=true",
    "coverage": "nyc report --reporter=lcov",
    "es5": "rm -rf ./lib && tsc -p tsconfig.json",
    "prepublishOnly": "npm run test && npm run es5",
    "ci": "npm run test && npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yusangeng/kekkai.git"
  },
  "keywords": [],
  "author": {
    "name": "yusangeng",
    "email": "yusangeng@outlook.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/dateformat": "^3.0.1",
    "@types/lodash": "^4.14.157",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "jsdom": "^16.3.0",
    "karma": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jsdom-launcher": "^8.0.2",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
